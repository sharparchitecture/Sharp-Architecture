<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>SharpArch.NHibernate</id>
    <version>$(SemanticVersion)</version>
    <authors>Sharp Architecture Dev Team</authors>
    <owners>Sharp Architecture Dev Team</owners>
    <licenseUrl>https://github.com/sharparchitecture/Sharp-Architecture/blob/master/License.txt</licenseUrl>
    <iconUrl>https://github.com/sharparchitecture/Sharp-Architecture/raw/master/Artefacts/Documentation/Logo_100x100.png</iconUrl>
    <projectUrl>http://sharparchitecture.github.io/</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Provides NHibernate persistance support for a Sharp Architecture application, providing NHibernate session managment and initialisation, MVC Transaction attributes and implementations of core SharpArch data access interfaces.</description>
    <summary>Architectural foundation for building maintainable applications with Domain Driven Design patterns and practices</summary>
    <language>en-US</language>
    <releaseNotes>https://github.com/sharparchitecture/Sharp-Architecture/releases/tag/$(SemanticVersion)</releaseNotes>
    <tags>sharp-architecture sharp-arch nhibernate</tags>
    <repository type="git" url="https://github.com/sharparchitecture/Sharp-Architecture.git" commit="$(CommitSHA)" />
    <dependencies>
      <group targetFramework="net461">
        <dependency id="FluentNhibernate" version="[2.1.2,3.0.0)" />
        <dependency id="NHibernate" version="[5.1.3,6.0.0)" />
        <dependency id="Newtonsoft.Json" version="[11.0.1,12.0.0)" />
        
        <dependency id="SharpArch.Domain" version="[$(SemanticVersion),$(NextMajorRelease))" />
      </group>
      <group targetFramework="netstandard2.0">
        <dependency id="FluentNhibernate" version="[2.1.2,3.0.0)" />
        <dependency id="NHibernate" version="[5.1.3,6.0.0)" />
        <dependency id="Newtonsoft.Json" version="[11.0.2,12.0)" />
        <dependency id="System.ComponentModel.Annotations" version="[4.5.0,5.0.0)" />
        <dependency id="System.Runtime.Serialization.Formatters" version="[4.3.0,5.0.0)" />
        
        <dependency id="SharpArch.Domain" version="[$(SemanticVersion),$(NextMajorRelease))" />
      </group>
    </dependencies>
  </metadata>
</package>